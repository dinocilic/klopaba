nput type=\"text\" name=\"linkedin\" v-model=\"this.linkedin\" @input=\"validateLinkedin(this.linkedin)\" class=\"form-input\" placeholder=\"https://www.linkedin.com/\" id=\"inputLinkedin\">\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            \r\n        </div>\r\n        \r\n        <!-- Adresa lokala lokala - Klopa.ba -->\r\n\r\n        <div class=\"form-row form-adresa\">\r\n            <div class=\"form-header\">\r\n                <h4 class=\"form-title\">\r\n                    <strong>Adresa Lokala: *</strong>\r\n                </h4>\r\n            </div>\r\n            <div class=\"form-box\">\r\n                <div class=\"form-group\">\r\n                    <div class=\"form-label\">\r\n                        <label for=\"adresa\">Ulica: </label>\r\n                        <span class=\"error-title\" v-if=\"errors.adresaError\">\r\n                            <small>\r\n                                {{ errors.adresaError }}\r\n                            </small>\r\n                        </span>\r\n                    </div>\r\n                    <div class=\"form-data\">\r\n                        <input type=\"text\" name=\"adresa\" v-model=\"this.adresa\" @input=\"validateUlicu(this.adresa)\" class=\"form-input\" placeholder=\"Puni naziv ulice lokala\" id=\"adresaInput\">\r\n                    </div>\r\n                </div>\r\n                <div class=\"form-group\">\r\n                    <div class=\"form-label\">\r\n                        <label for=\"grad\">Grad:</label>\r\n                        <span class=\"error-title\" v-if=\"errors.gradError\">\r\n                            <small>\r\n                                {{ errors.gradError }}\r\n                            </small>\r\n                        </span>\r\n                    </div>\r\n                    <div class=\"form-data\">\r\n                        <select v-model=\"this.grad\" @change=\"validateGrad(this.grad)\" name=